<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#283E51"
    tools:context=".SignUpActivity">


    <TextView
        android:id="@+id/myText"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:fontFamily="cursive"
        android:paddingTop="15dp"
        android:text="SignUp To SarcoPixel"
        android:textAlignment="center"
        android:textColor="#F8F8FF"
        android:textFontWeight="900"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.006" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="170dp"
        android:layout_height="65dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:background="@drawable/text_field"
        android:fontFamily="monospace"
        android:textColor="#fff"
        android:hint="First Name"
        android:paddingLeft="15dp"
        android:textColorHint="#F8F8FF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myText"
        app:layout_constraintVertical_bias="0.473" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="170dp"
        android:layout_height="65dp"
        android:background="@drawable/text_field"
        android:inputType="textPersonName"
        android:ems="10"
        android:fontFamily="monospace"
        android:textColor="#fff"
        android:hint="Other Name"
        android:paddingLeft="15dp"
        android:textColorHint="#F8F8FF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.348"
        app:layout_constraintStart_toEndOf="@+id/editText1"
        app:layout_constraintTop_toBottomOf="@+id/myText"
        app:layout_constraintVertical_bias="0.473" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="355dp"
        android:layout_height="65dp"
        android:layout_marginTop="84dp"
        android:background="@drawable/text_field"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="Your Email Address"
        android:inputType="textEmailAddress"
        android:paddingLeft="15dp"
        android:textColorHint="#F8F8FF"
        android:textColor="#fff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myText"
        app:layout_constraintVertical_bias="0.006" />

    <EditText
        android:id="@+id/editTextPhone3"
        android:layout_width="355dp"
        android:layout_height="65dp"
        android:background="@drawable/text_field"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="Phone Number"
        android:inputType="phone"
        android:paddingLeft="15dp"
        android:textColorHint="#F8F8FF"
        android:textColor="#fff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3"
        app:layout_constraintVertical_bias="0.029" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="355dp"
        android:layout_height="65dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:background="@drawable/text_field"
        android:fontFamily="monospace"
        android:hint="Your Admission No."
        android:paddingLeft="15dp"
        android:textColorHint="#F8F8FF"
        android:textColor="#fff"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone3"
        app:layout_constraintVertical_bias="0.037" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="355dp"
        android:layout_height="65dp"
        android:background="@drawable/text_field"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="Create A Password"
        android:inputType="textPassword"
        android:paddingLeft="15dp"
        android:textColor="#fff"
        android:textColorHint="#F8F8FF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName4"
        app:layout_constraintVertical_bias="0.052" />

    <EditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="355dp"
        android:layout_height="65dp"
        android:background="@drawable/text_field"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="Confirm Password"
        android:inputType="textPassword"
        android:paddingLeft="15dp"
        android:textColor="#fff"
        android:textColorHint="#F8F8FF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editText2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
        app:layout_constraintVertical_bias="0.075" />

    <EditText
        android:id="@+id/editTextPhone4"
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:background="@drawable/text_field"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="Serial Code"
        android:inputType="phone"
        android:paddingLeft="15dp"
        android:textColor="#fff"
        android:textColorHint="#F8F8FF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword3"
        app:layout_constraintVertical_bias="0.08" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:background="@drawable/text_field"
        android:ems="10"
        android:fontFamily="monospace"
        android:hint="NickName"
        android:textColor="#fff"
        android:inputType="textPersonName"
        android:paddingLeft="15dp"
        android:textColorHint="#F8F8FF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toEndOf="@+id/editTextPhone4"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword3"
        app:layout_constraintVertical_bias="0.08" />

    <Button
        android:id="@+id/button2"
        android:layout_width="355dp"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/my_btn"
        android:text="Sign Up"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone4"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="Already Have An Account?"
        android:textColor="#F8F8FF"
        android:textFontWeight="900"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.284"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Log In"
        android:textColor="#BDEDFF"
        android:textFontWeight="900"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>